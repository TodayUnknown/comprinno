TWO numbers

#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;

int main() 
{
	int t;
	cin>>t;
	while(t--)
	{
	    ll a, b, n;
	    cin>>a>>b>>n;
	    
	    if(n%2==0)
	    {
	        if(a>b)
    	    {
    	        cout<<(a/b)<<endl;
    	    }
    	    else
    	    {
    	        cout<<(b/a)<<endl;
    	    }
	    }
	    else
	    {
	        if((2*a)>b)
    	    {
    	        cout<<((2*a)/b)<<endl;
    	    }
    	    else
    	    {
    	        cout<<(b/(2*a))<<endl;
    	    }
	    }
	}
}
